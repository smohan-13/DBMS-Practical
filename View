mysql> create table course (courseid int auto_increment primary key, course_name varchar(100) not null unique, credit int not null check (credit > 0));
Query OK, 0 rows affected (0.03 sec)

mysql> drop table if exists enrollments;
Query OK, 0 rows affected (0.02 sec)

mysql> create table enrollments (eid int auto_increment primary key, enno int not null, courseid int not null, year int not null, foreign key (enno) references student(enno) on delete cascade, foreign key (courseid) references course(courseid) on delete cascade);
Query OK, 0 rows affected (0.04 sec)

mysql> insert into course (course_name,credit) values ('data structures',4);
