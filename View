 MySQL  localhost:33060+ ssl  university_database  SQL > create view env as
                                                      -> select s.name, c.course_name, e.year
                                                      -> from student s
                                                      -> join enrollments e on s.eno = e.eno
                                                      -> join course c on e.courseid = c.courseid;
