mysql> create table course (courseid int primary key, course_name varchar(100), credit int);
Query OK, 0 rows affected (0.03 sec)

mysql> insert into course (courseid, course_name, credit) values (101, 'mathematics', 4), (102, 'physics', 3), (103, 'chemistry', 3), (104, 'computer science', 4), (105, 'english', 2);
Query OK, 5 rows affected (0.01 sec)
Records: 5  Duplicates: 0  Warnings: 0

mysql> create table enrollments (eid int primary key, eno int, courseid int, year int, foreign key (eno) references student(eno), foreign key (courseid) references course(courseid));
Query OK, 0 rows affected (0.04 sec)

mysql> insert into enrollments (eid, eno, courseid, year) values (1, 1, 101, 2024), (2, 2, 102, 2024), (3, 3, 103, 2025), (4, 4, 104, 2025), (5, 5, 105, 2024);
Query OK, 5 rows affected (0.01 sec)
Records: 5  Duplicates: 0  Warnings: 0

mysql> create view studentview1 as select eno, name from student;
